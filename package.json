{
  "name": "h5p-xapi-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/entry.ts",
    "dev": "nodemon src/entry.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha --require ts-node/register tests/unit/**/*.test.ts",
    "test:integration": "mocha --require ts-node/register tests/integration/**/*.test.ts --timeout 8000",
    "test:single": "mocha --require ts-node/register --grep \"calculate A\" tests/**/*.test.ts",
    "test:file": "mocha --require ts-node/register tests/unit/stub.test.ts",
    "example": "ts-node src/firehoseExample.ts",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc src/**/*.ts --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/calmisland/h5p-xapi-server.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/calmisland/h5p-xapi-server#readme",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/cookie": "^0.4.0",
    "@types/elasticsearch": "^5.0.37",
    "@types/express": "^4.17.11",
    "@types/geoip-lite": "^1.4.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.4",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@aws-sdk/client-firehose": "^3.8.1",
    "@elastic/elasticsearch": "^7.11.0",
    "@graphql-tools/graphql-file-loader": "^6.2.7",
    "@graphql-tools/load": "^6.2.7",
    "apollo-server-express": "^2.21.0",
    "cookie": "^0.4.1",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.2",
    "graphql": "^15.5.0",
